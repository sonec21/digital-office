{
  "enabled": true,
  "interval_minutes": 30,
  "max_items_per_run": 1,
  "min_priority": "medium",
  "branch_prefix": "auto/improve",
  "auto_merge": false,
  "kill_switch_env": "AUTO_IMPROVE_ENABLED",
  "allowlist_paths": [
    "src/app/office/",
    "brain/",
    "README_OFFICE.md",
    ".learnings/"
  ],
  "denylist_paths": [
    ".env",
    "caddy",
    "infra",
    "deploy",
    "systemd",
    "auth",
    "secrets"
  ]
}
